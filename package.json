{
    "type": "module",
    "scripts": {
        "test": "node --test --loader ts-node/esm tests/*.spec.mts",
        "fmt": "prettier -w .",
        "lint": "prettier --check .",
        "build": "rollup --config rollup.config.js",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@swc/core": "^1.3.40",
        "@types/node": "^18.15.3",
        "husky": "^8.0.3",
        "lint-staged": "^13.2.0",
        "prettier": "^2.8.4",
        "rollup": "^3.19.1",
        "rollup-plugin-dts": "^5.2.0",
        "rollup-plugin-swc": "^0.2.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "name": "icalendar.js",
    "description": "Modern module to parse and stringify iCalendar to Javascript.",
    "version": "0.1.6",
    "author": "Jonathan Delgado <hi@jon.soy> (https://jon.soy)",
    "exports": {
        ".": {
            "require": "./dist/icalendar.cjs",
            "import": "./dist/icalendar.mjs"
        },
        "./lexer": {
            "require": "./dist/lexer.cjs",
            "import": "./dist/lexer.mjs"
        },
        "./ast": {
            "require": "./dist/ast.cjs",
            "import": "./dist/ast.mjs"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "./types.d.ts"
            ]
        }
    },
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT",
    "lint-staged": {
        "*.{mts,json,md}": "prettier --write"
    },
    "keywords": [
        "icalendar",
        "ics",
        "RFC2445",
        "RFC5545"
    ]
}
